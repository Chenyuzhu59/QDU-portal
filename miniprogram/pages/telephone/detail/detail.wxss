page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  font-family: "Arial"
}

.container .title { 
  width: 88%;
  margin-top: 0rpx;
  background: white;
  border-radius: 15rpx;
  border: 2px solid #fff;
  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);
}

.container .show { 
  width: 88%;
  /* margin-left: 3%; */
  margin-top: 30rpx;
  background: white;
  border-radius: 15rpx;
  border: 2px solid #fff;
  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);
}

.container .notes{
  width: 88%;
  margin-top: 50rpx;
}